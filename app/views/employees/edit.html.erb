<h1>Editing details</h1>
 
<%= simple_form_for :@employee do |f| %>
 
  

  


  <p>
    <%= f.input :first_name %>
    <%= f.input :last_name  %>
    <%= f.input :email %>
    <%= f.input :password  %>
    <%= f.input :confirm_password %>
    <%= f.input :date_of_birth %>
    <%= f.input :adress %>
    <%= f.input :phone_number  %>
    <%= f.input :pincode %>
    
  
    
   
    
    
   
  
    
  </p>
   

 
  <p>
    <%= f.submit %>
  </p>
<% end %>
  
 
<%= link_to 'Back', employees_path %>